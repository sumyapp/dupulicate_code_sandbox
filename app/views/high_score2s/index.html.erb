<p id="notice"><%= notice %></p>

<h1>High Score2s</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @high_score2s.each do |high_score2| %>
      <tr>
        <td><%= high_score2.game %></td>
        <td><%= high_score2.score %></td>
        <td><%= link_to 'Show', high_score2 %></td>
        <td><%= link_to 'Edit', edit_high_score2_path(high_score2) %></td>
        <td><%= link_to 'Destroy', high_score2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New High Score2', new_high_score2_path %>
